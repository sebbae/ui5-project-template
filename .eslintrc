{
	"root": true,
	"env": {
		"browser": true,
		"worker": true
	},
	"globals": {
		"sap": true,
		"jQuery": true
	},
	"parserOptions": {
		"ecmaVersion": 6
	},
	"ecmaFeatures": {
		"templateStrings": true,
		"blockBindings": true
	},
	"rules": {
		"brace-style": [2, "1tbs", { "allowSingleLine": true }],
		"consistent-this": [2, "that"],
		"no-div-regex": 2,
		"no-floating-decimal": 2,
		"no-mixed-spaces-and-tabs": [2, true],
		"no-nested-ternary": 2,
		"no-redeclare": 2,
		"no-self-compare": 2,
		"no-undef": 2,
		"no-unused-vars": [2, {"vars":"all", "args":"none"}],
		"quotes": [2, "double", "avoid-escape"],
		"radix": 2,
		"keyword-spacing": 2,
		"space-unary-ops": 2,
		"strict": 2,
		"wrap-iife": [2, "any"],

		"block-scoped-var": 1,
		"camelcase": 1,
		"consistent-return": 1,
		"max-nested-callbacks": [1, 3],
		"new-cap": [1, {"capIsNewExceptions": ["jQuery.Deferred"]}],
		"no-dupe-args": 1,
		"no-dupe-keys": 1,
		"no-duplicate-case": 1,
		"no-empty": 1,
		"no-extra-boolean-cast": 1,
		"no-new": 1,
		"no-new-wrappers": 1,
		"no-throw-literal": 1,
		"no-use-before-define": [1, "nofunc"],
		"no-warning-comments": 1,
		"semi": 1,

		"comma-spacing": 0,
		"default-case": 0,
		"dot-notation": 0,
		"eqeqeq": 0,
		"key-spacing": 0,
		"no-lonely-if": 0,
		"no-multi-spaces": 0,
		"no-shadow": 0,
		"no-underscore-dangle": 0,
		"no-unused-expressions": 0,
		"valid-jsdoc": 0
	}
}
