<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	controllerName="my.app.main.samples.Samples"
	>

	<Page showFooter="false" showHeader="false" showSubHeader="true">
		<content>
			<ScrollContainer class="sapUiSmallMarginBeginEnd">
				<List headerText="{i18n>SAMPLES_LIST}" items="{od>/Samples}" >
					<StandardListItem title="{od>Value}" counter="1"/>
				</List>
			</ScrollContainer>
		</content>
	</Page>
</mvc:View>
